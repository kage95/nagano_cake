<table class="table table-hover">
  <thead>
    <tr>
      <th class="font-weight-bold">
        ジャンル検索
      </th>
    </tr>
  </thead>
  <tbody>
    <% genres.each do |genre| %>
    <tr>
      <td class="text-dark text-decoration-none">
        <%= link_to genre_search_path(genre_id: genre.id),class:"text-dark text-decoration-none" do %>
          <%= genre.name %>
        <% end%>
      </td>
    </tr>
    <% end %> 
  </tbody>
</table>
